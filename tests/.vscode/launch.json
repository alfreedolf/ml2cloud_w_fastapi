    "version": "0.2.0",
    "configurations": [
        {
            "name": "Unit Test Model",
            "type": "debugpy",
            "request": "launch",
            "program": "unit/model_test.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}${pathSeparator}${env:PYTHONPATH}"
            },
            "args": ["-k", "model_test"]
        },
        {
            "name": "Unit Test API",
            "type": "debugpy",
            "request": "launch",
            "program": "unit/api_test.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}${pathSeparator}${env:PYTHONPATH}"
            }
            ,
            "args": ["-k", "api_test"]
        }
    ]
}